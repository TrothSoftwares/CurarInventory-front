<div class="print-box">
<h2 class="ui left floated header no-print"> Inventory Report</h2>
<img src="images/z2.jpg" class="only-print" width="200px">
<div class="ui clearing  divider only-print"></div>
<div class="ui center aligned dividing header only-print">Inventory Report</div>
<button class="ui tiny primary right floated button no-print" {{action "printReport"}}>Print</button>
{{pikaday-input  placeholder="Select Date" value=startdate}}
{{pikaday-input  placeholder="Select Date" value=enddate}}
<table class="ui celled structured center aligned table">
  <thead>
    <tr>
    <th>No.</th>
    <th>Item Code</th>
    <th>Product Name</th>
    <th>Stock In Hand </th>
    <th>Retail Price</th>
    <th>Total (Retail)</th>

  </tr></thead>
  <tbody>
  {{#each products  as |product index|}}
  <tr>
    <td> {{ incrementone index}}</td>
<td> {{product.itemcode}}</td>
<td> {{product.productname}}</td>
<td> {{product.initialstocklevel}}</td>
<td> {{product.retailprice}}</td>
<td> {{product.computedtotal_retail_intialstocklevel}}</td>
  </tr>
  {{/each}}

  </tbody>

</table>
</div>
{{outlet}}
